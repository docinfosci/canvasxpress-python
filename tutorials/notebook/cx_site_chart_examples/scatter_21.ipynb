    render_to="scatter2d1",
    data={
        "y": {
            "smps": [
                "Alcohol",
                "Tobacco"
            ],
            "vars": [
                "North",
                "Yorkshire",
                "Northeast",
                "East Midlands",
                "West Midlands",
                "East Anglia",
                "Southeast",
                "Southwest",
                "Wales",
                "Scotland",
                "Northern Ireland"
            ],
            "data": [
                [
                    6.47,
