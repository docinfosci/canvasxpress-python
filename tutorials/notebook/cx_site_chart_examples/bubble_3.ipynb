                35,
                40,
                40,
                35,
                30,
                25,
                20,
                15,
                10,
                5
            ]
        }
    },
    config={
        "circularRotate": 45,
        "circularType": "bubble",
        "graphType": "Circular",
        "hierarchy": [
            "Level1",
            "Level2",
            "Level3"
        ],
        "title": "Hierarchical Bubble Graph"
    },
    width=500,
    height=500,
    events=CXEvents(),
    after_render=[
        [
            "setDimensions",
            [
                613,
